{
    "name": "joggapp/laravel-natural-language",
    "description": "Laravel package for the Google Natural language API",
    "keywords": [
        "laravel",
        "package",
        "google",
        "natural language",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Harish Toshniwal",
            "email": "harish@jogg.co"
        }
    ],
    "autoload": {
        "psr-4": {
            "JoggApp\\NaturalLanguage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoggApp\\NaturalLanguage\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "google/cloud-language": "^0.28",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoggApp\\NaturalLanguage\\NaturalLanguageServiceProvider"
            ],
            "aliases": {
                "NaturalLanguage": "JoggApp\\NaturalLanguage\\NaturalLanguageFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
